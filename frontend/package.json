{
  "name": "frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "Track Your Regions Frontend - Next Gen",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "test": "node ../scripts/test-report.mjs frontend-unit",
    "test:watch": "vitest",
    "test:coverage": "node ../scripts/test-report.mjs frontend-coverage",
    "test:e2e": "playwright test",
    "test:e2e:smoke": "node ../scripts/test-report.mjs e2e-smoke",
    "test:e2e:full": "node ../scripts/test-report.mjs e2e-full",
    "knip": "knip --include files,dependencies,unlisted",
    "knip:full": "knip"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@react-buddy/ide-toolbox": "^2.4.0",
    "@tanstack/react-query": "^5.60.0",
    "@tanstack/react-virtual": "^3.10.0",
    "@turf/turf": "^7.3.2",
    "jwt-decode": "^4.0.0",
    "maplibre-gl": "^4.7.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-map-gl": "^7.1.0",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-security": "^3.0.1",
    "jsdom": "^26.0.0",
    "knip": "^5.84.1",
    "playwright": "^1.58.0",
    "typescript": "^5.6.0",
    "vite": "^5.4.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
